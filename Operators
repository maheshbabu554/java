package Java;

public class Operators {

	public static void main(String[] args) {
		int a=10;
		int b=20;
			int c=a+b;
			System.out.println(c);
		// TODO Auto-generated method stub
			int d=a*b;
			int e=a-b;
			int f=a/b;
			int g=b%a;
			System.out.println(d);
			System.out.println(e);
			System.out.println(f);
			System.out.println(g);


	}

}
