package Java;

public class Ifelse2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int num=12;
if(num%2==0) {
	System.out.println("Even");
	
}
else {
	System.out.println("odd");

}

int age=23;
if(age>=18) {
	System.out.println("Eligible for a vote");
}
else {
	System.out.println("Not Eligable for Vote bcz he/she is minor ");

}
	}

}
